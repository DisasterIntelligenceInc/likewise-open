make()
{
    NSS_SOURCES="\
	nss-group.c \
	nss-main.c \
	nss-user.c \
	nss-pruser.c"

    mk_multiarch_do
        NSSEXT=.1
        mk_dlo \
            DLO=libnss_lsass \
            EXT="$NSSEXT" \
            INSTALLDIR="$NSSDIR" \
            SOURCES="$NSS_SOURCES" \
            GROUPS="../common/common" \
            INCLUDEDIRS="../include ../../auth/include ../../../include" \
            HEADERDEPS="lwadvapi.h" \
            LIBDEPS="lsaclient sec"
    mk_multiarch_done
}
