make()
{
    PARSE_SOURCES="\
        regio.c \
        reglex.c \
        regiconv.c \
        regparse.c"

    mk_group \
        GROUP=regparse \
        SOURCES="$PARSE_SOURCES" \
        INCLUDEDIRS=". ../include" \
        LIBDEPS="regcommon lwbase lwbase_nothr" \
        HEADERDEPS="lwmsg/lwmsg.h lw/base.h iconv.h"
}