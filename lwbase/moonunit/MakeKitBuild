make()
{
    TEST_SOURCES="test-security.c test-atomic.c test-threadpool.c test-compare.c test-convert.c test-printf.c"

    mk_group \
        GROUP="benchmark" \
        SOURCES="benchmark.c" \
        INCLUDEDIRS=". ../include" \
        LIBDEPS="lwbase lwbase_nothr"

    mk_moonunit \
        DLO="lwbase_mu" \
        SOURCES="$TEST_SOURCES" \
        GROUPS="benchmark" \
        INCLUDEDIRS=". ../include"

    mk_program \
        PROGRAM=benchmark \
        INSTALL=no \
        SOURCES="benchmark-main.c" \
        INCLUDEDIRS=". ../include" \
        GROUPS="benchmark"

    mk_add_all_target "$result"
}