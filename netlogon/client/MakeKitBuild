make()
{
    CLIENT_SOURCES="\
        dcinfo.c \
        ipc_client.c \
        krb5env.c"

    mk_library \
        LIB=lwnetclientapi \
        SOURCES="$CLIENT_SOURCES" \
        INCLUDEDIRS=". ../include" \
        HEADERDEPS="lwmsg/lwmsg.h lwadvapi.h reg/lwreg.h" \
        LIBDEPS="lwnetcommon"
}