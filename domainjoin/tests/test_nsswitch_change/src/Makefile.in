BUILD_ROOT = ../../..

SHELL= /bin/sh
VPATH= @srcdir@

subdirs= @subdirs@
top_srcdir= @top_srcdir@
srcdir = @srcdir@
prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = ${exec_prefix}/bin
sbindir = ${exec_prefix}/sbin
infodir = ${prefix}/info
libdir = ${prefix}/lib
mandir = ${prefix}/man/man1

DESTDIR=@DESTDIR@
CC = @CC@
CPPFLAGS = @CPPFLAGS@
CFLAGS = $(CPPFLAGS) @CFLAGS@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@ @LIBXML2_LDFLAGS@
INSTALL = @INSTALL@

PUBLIC_INCLUDE_DIR = $(BUILD_ROOT)/include
DOMAINJOIN_DIR = $(BUILD_ROOT)/libdomainjoin

DOMAINJOIN_LIB = $(DOMAINJOIN_DIR)/src/libdomainjoin.a

INCLUDES = -I$(PUBLIC_INCLUDE_DIR) -I$(DOMAINJOIN_DIR)/include
CFLAGS += $(INCLUDES) -D__DO_NOT_DEFINE_COMPILE @CENTUTILS_CPPFLAGS@
LDFLAGS += @CENTUTILS_LDFLAGS@

LIBS += -Bdynamic -L$(DOMAINJOIN_DIR)/src -ldomainjoin

OBJS = main.o

PROGRAM = test_nsswitch_change

all : $(PROGRAM)

$(PROGRAM) : $(OBJS) $(DOMAINJOIN_LIB)
	$(CC) -o $@ $(OBJS) $(LDFLAGS) @CENTUTILS_LIBS@ $(LIBS)

$(OBJS) : %.o : %.c
	$(CC) -c $(CFLAGS) $< -o $@

clean :
	$(RM) $(OBJS) $(PROGRAM)

distclean:
	$(RM) Makefile

install: all


uninstall:


