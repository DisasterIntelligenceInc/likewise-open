#!/bin/sh

PREFIX_DIR=@prefix@

export LD_LIBRARY_PATH
LD_LIBRARY_PATH=${PREFIX_DIR}/lib:${LD_LIBRARY_PATH}

#S7 Changes begin
osType=`uname`
if [ "SunOS" = "$osType" ]; then
	export PATH
	PATH=/opt/csw/bin:${PATH}
fi
#S7 Changes Ends

umask 022
exec ${PREFIX_DIR}/bin/lwi-domainjoin-cli "$@"
