# Breakpoints regex
grep '\.pfn' samdbinit.c | sed -e 's/.*= &//' -e 's/,//' -e 's/^/b /'
grep '\.pfn' vmdirdbinit.c | sed -e 's/.*= &//' -e 's/,//' -e 's/^/b /'


int ldap_add_ext(
       LDAP *ld,
       const char *dn,
       LDAPMod **attrs,
       LDAPControl **sctrls,
       LDAPControl **cctrls,
       int *msgidp );

#if LDAP_DEPRECATED
LDAP_F( int )
ldap_add LDAP_P((       /* deprecated, use ldap_add_ext */
        LDAP *ld,
        LDAP_CONST char *dn,
        LDAPMod **attrs ));




