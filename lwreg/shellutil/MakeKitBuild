make()
{
    RSUTILS_SOURCES="\
        regutilwrap.c \
        rsutils.c \
        import.c \
        export.c"

    mk_library \
        LIB=rsutils \
        SOURCES="$RSUTILS_SOURCES" \
        INCLUDEDIRS=". ../include" \
        LIBDEPS="regclient" \
        HEADERDEPS="lw/base.h lwmsg/lwmsg.h"
}
