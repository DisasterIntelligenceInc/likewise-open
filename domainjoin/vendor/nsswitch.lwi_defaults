passwd:       files
group:        files
hosts:        dns [NOTFOUND=return] files
ipnodes:      dns [NOTFOUND=return] files
networks:     files
protocols:    files
rpc:          files
publickey:    files
netgroup:     files
automount:    files 
aliases:      files 
services:     files
