make()
{
    mk_program \
	PROGRAM=lsassd \
	INSTALLDIR="${MK_SBINDIR}" \
	SOURCES="main.c globals.c listener.c libmain.c backtrace.c sighandler.c" \
	INCLUDEDIRS=". ../include ../../include" \
	HEADERDEPS="lwadvapi.h lwio/lwio.h" \
	LIBDEPS="lsaserverapi ntlmserver lsaclient_ntlm lwnetclientapi lwnetcommon lwioclient lwiocommon lwmsg lwmsg_nothr lwadvapi lwadvapi_nothr dcerpc"
}