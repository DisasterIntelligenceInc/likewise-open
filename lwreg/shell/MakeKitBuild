make()
{
    mk_program \
        PROGRAM=lwregshell \
        SOURCES="cmdparse.c regshell.c" \
        GROUPS="../parse/regparse.og" \
        INCLUDEDIRS=". ../include ../shellutil" \
        LIBDEPS="regclient rsutils edit" \
        HEADERDEPS="lw/base.h lwmsg/lwmsg.h histedit.h"

    mk_install_file \
        INSTALLFILE="${MK_BINDIR}/lw-edit-reg" \
        FILE="lw-edit-reg.sh"
}